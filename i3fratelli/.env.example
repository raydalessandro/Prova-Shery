<artifact identifier="env-example" type="application/vnd.ant.code" language="bash" title=".env.example - Template variabili ambiente">
# AMBIENTE
NODE_ENV=production
REACT_APP_ENV=production
REACT_APP_VERSION=3.1.0
URL BASE
REACT_APP_BASE_URL=https://www.i3fratelli.it
REACT_APP_API_URL=https://api.i3fratelli.it
GOOGLE SERVICES
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
REACT_APP_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
REACT_APP_GTM_ID=GTM-XXXXXXX
REACT_APP_GOOGLE_SITE_VERIFICATION=your_verification_code_here
GOOGLE MY BUSINESS
REACT_APP_GMB_PLACE_ID=your_place_id_here
REACT_APP_GMB_API_KEY=your_gmb_api_key_here
REACT_APP_GMB_REVIEWS_WIDGET_ID=your_elfsight_widget_id
WHATSAPP
REACT_APP_WHATSAPP_NUMBER=393331234567
REACT_APP_WHATSAPP_DEFAULT_MESSAGE=Salve, vorrei ordinare del pane
REACT_APP_WHATSAPP_B2B_MESSAGE=Salve, sono un ristorante e vorrei informazioni su forniture B2B
SOCIAL MEDIA
REACT_APP_FACEBOOK_URL=https://www.facebook.com/i3fratelli
REACT_APP_INSTAGRAM_URL=https://www.instagram.com/i3fratelli
REACT_APP_TIKTOK_URL=https://www.tiktok.com/@i3fratelli
EMAIL E FORM
REACT_APP_CONTACT_EMAIL=info@i3fratelli.it
REACT_APP_B2B_EMAIL=ristoranti@i3fratelli.it
REACT_APP_FORMSPREE_ID=your_formspree_id_here
ORARI (per override dinamico se necessario)
REACT_APP_IS_OPEN_24H=true
REACT_APP_EMERGENCY_PHONE=+39333456789
FEATURE FLAGS
REACT_APP_ENABLE_BLOG=true
REACT_APP_ENABLE_B2B=true
REACT_APP_ENABLE_AI_RESONANCE=true
REACT_APP_ENABLE_COOKIE_BANNER=true
REACT_APP_ENABLE_WHATSAPP_FLOAT=true
PERFORMANCE
REACT_APP_IMAGE_CDN_URL=https://cdn.i3fratelli.it
REACT_APP_ENABLE_SERVICE_WORKER=true
REACT_APP_ENABLE_LAZY_LOADING=true
SECURITY
REACT_APP_CSP_REPORT_URI=https://i3fratelli.report-uri.com/r/d/csp/enforce
REACT_APP_ENABLE_RATE_LIMITING=true
SEO
REACT_APP_DEFAULT_META_TITLE=I 3 Fratelli - Panificio H24 Milano | Sempre Aperti
REACT_APP_DEFAULT_META_DESCRIPTION=L'unico vero panificio di quartiere aperto 24 ore su 24 a Milano. Pane artigianale con lievito madre, tradizione e mani di granite. Consegne per ristoranti.
REACT_APP_DEFAULT_OG_IMAGE=https://www.i3fratelli.it/og-image.jpg
MONITORING
REACT_APP_SENTRY_DSN=your_sentry_dsn_here
REACT_APP_ENABLE_ERROR_TRACKING=true
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true
SVILUPPO
REACT_APP_ENABLE_DEBUG=false
REACT_APP_SHOW_AI_TRAFFIC=true
</artifact>
</artifacts>
